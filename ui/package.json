{
  "name": "kore-ui",
  "version": "0.1.0",
  "scripts": {
    "dev": "nodemon",
    "test": "jest --verbose --testPathPattern __tests__/unit",
    "test-watch": "npm test -- --watch",
    "test-e2e": "jest --verbose --preset jest-puppeteer --testSequencer ./__tests__/e2e/sequencer.js --runInBand --testMatch **/__tests__/e2e/*.test.js --testPathIgnorePatterns nightly",
    "test-e2e-nightly-gke": "jest --verbose --preset jest-puppeteer --testSequencer ./__tests__/e2e/sequencer.js --runInBand --testMatch **/__tests__/e2e/nightly/gke.test.js",
    "test-e2e-nightly-eks": "jest --verbose --preset jest-puppeteer --testSequencer ./__tests__/e2e/sequencer.js --runInBand --testMatch **/__tests__/e2e/nightly/eks.test.js",
    "build": "next build",
    "start": "NODE_ENV=production node server/index.js",
    "lint": "eslint ."
  },
  "dependencies": {
    "@zeit/next-less": "^1.0.1",
    "antd": "^3.25.1",
    "axios": "^0.19.0",
    "babel-plugin-import": "^1.12.2",
    "babel-plugin-import-directory": "^1.1.0",
    "body-parser": "^1.19.0",
    "connect-ensure-login": "^0.1.1",
    "connect-redis": "^4.0.3",
    "cookie-parser": "^1.4.4",
    "express": "^4.17.1",
    "express-react-views": "^0.11.0",
    "express-session": "^1.17.0",
    "inflect": "^0.4.1",
    "js-yaml": "^3.14.0",
    "less": "^3.10.3",
    "less-vars-to-js": "^1.3.0",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "next": "^9.4.4",
    "null-loader": "^3.0.0",
    "openid-client": "^3.10.0",
    "passport": "^0.4.0",
    "prop-types": "^15.7.2",
    "react": "^16.10.1",
    "react-dom": "^16.10.1",
    "react-markdown": "^4.3.1",
    "redis": "^2.8.0",
    "swagger-client": "^3.10.0"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.3",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.15.1",
    "eslint": "^6.7.2",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-jest": "^23.1.1",
    "eslint-plugin-react": "^7.17.0",
    "jest": "^24.9.0",
    "jest-puppeteer": "^4.4.0",
    "jest-when": "^2.7.0",
    "mini-css-extract-plugin": "^0.8.0",
    "nock": "^12.0.3",
    "nodemon": "^1.19.4",
    "puppeteer": "^3.3.0",
    "react-test-renderer": "^16.12.0"
  },
  "license": "Apache-2.0"
}
